<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <% @traces.each do |trace| %>
    <Placemark>
      <name><%=h trace.file_name %></name>
      <LineString>
        <coordinates>
        <% trace.trace_points.each do |point| %>
          <%=h point.lon %>,<%=h point.lat -%>
        <% end %>
        </coordinates>
      </LineString>
    </Placemark>
    <% end %>
  </Document>
</kml>